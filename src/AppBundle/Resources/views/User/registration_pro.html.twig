                                <i class="fa fa-user"></i>
                            </button>
                        </div>
                    </div>
                </div>
                <!-- Main -->
                <div class="main" role="main">

                    <!-- Page Heading -->
                    <section class="page-heading">
                        <div class="container">
                            <div class="row">
                                <div class="col-md-12">
                                    <h1>S'inscrire</h1>
                                </div>
                            </div>
                        </div>
                    </section>
                    <!-- Page Heading / End -->

                    <!-- Page Content -->

                        <section class="page-content">
                            <div class="container">

                                <div class="row">
                                    <center>
                                    <div class="col-md-6">
                                        <div class="spacer-lg visible-sm visible-xs"></div>
                                        <div class="box">
                                            <h3>s'inscrire</h3>
                                            {{ form_start(form) }}
                                            {{ form_end(form) }}
                                        </div>
                                    </div>
                                    </center>
                                </div>

                            </div>
                        </section>


                    <!-- Page Content / End -->

                    <!-- Footer -->


                </div>
